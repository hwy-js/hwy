{
  "name": "hwy-cf-test-2",
  "private": true,
  "type": "module",
  "scripts": {
    "hwy-prebuild": "tailwindcss -i src/styles/tw-input.css -o src/styles/tw-output.bundle.css",
    "build": "tsc --noEmit && hwy-build && hwy-cf-pages-hack",
    "start": "node dist/main.js",
    "dev:serve": "hwy-dev-serve",
    "dev:wrangler": "npm run build && wrangler pages dev ./dist --compatibility-flag=\"nodejs_compat\" --port=3491",
    "dev": "npm-run-all --parallel dev:*"
  },
  "dependencies": {
    "@hono/node-server": "^1.2.0",
    "hono": "^3.7.5",
    "hwy": "workspace:*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231002.0",
    "@hwy-js/dev": "workspace:*",
    "@types/node": "^20.8.4",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.4",
    "htmx.org": "^1.9.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "wrangler": "^3.11.0"
  },
  "engines": {
    "node": ">=18.14.1"
  }
}
