    "preact-render-to-string": "^6.3.1"
  },
  "devDependencies": {
    "@hwy-js/build": "workspace:*",
    "@hwy-js/client": "workspace:*",
    "@hwy-js/dev": "workspace:*",
    "@preact/signals": "^1.2.2",
    "@types/node": "^20.10.4",
    "@types/nprogress": "^0.2.3",
    "history": "^5.3.0",
    "nprogress": "^0.2.0",
    "preact": "^10.19.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.14.1"
  }
}
