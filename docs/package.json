{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "vercel-build": "tsc && hwy-build",
    "start": "node dist/main.js",
    "dev": "hwy-dev-serve",
    "to-latest": "node ./scripts/to-latest.js && pnpm i && npm i",
