{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && hwy-build",
    "start": "node dist/main.js",
    "dev": "hwy-dev-serve",
    "to-latest": "node ./scripts/to-latest.js && pnpm i && npm i",
    "to-workspace": "node ./scripts/to-workspace.js && pnpm i",
    "update-latest": "pnpm update --latest",
    "nuke": "rm -rf dist && rm -rf public/dist"
  },
  "dependencies": {
    "devalue": "^4.3.2",
    "hwy": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@hwy-js/build": "workspace:*",
    "@hwy-js/client": "workspace:*",
    "@hwy-js/dev": "workspace:*",
    "@types/node": "^20.11.27",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "nprogress": "^0.2.0",
    "typescript": "^5.4.2"
  }
}
